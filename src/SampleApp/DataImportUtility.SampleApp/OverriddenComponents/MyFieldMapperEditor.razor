@using DataImportUtility.Models
@using DataImportUtility.SampleApp.Client.Models
@using DataImportUtility.SampleApp.Models

@inherits FieldMapperEditorBase

<div class="field-mapper-editor-wrapper">
    @base.Render
</div>

@code {
    protected override RenderFragment ItemContentTemplate(FieldMapping curField) =>
        curField.FieldName != nameof(TargetRecord.IgnoreField1)
        ? base.ItemContentTemplate(curField)
        : 
        @<tr>
            <td>I'm Overridden</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
        </tr>;
}
